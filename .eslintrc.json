{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error",
      {
        "printWidth": 180,
        "trailingComma": "all",
        "singleQuote": true,
        "endOfLine": "lf",
        "semi": true,
        "tabWidth": 2
      }
    ]
  }
}
